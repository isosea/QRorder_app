<% if order.quantity != 0 %>
<ul class="list-group">
  <li class="list-group-item">
    <div class="form-check">
      <input class="form-check-input" type="checkbox" value="order-check" id="flexCheckDefault">
      <label class="form-check-label" for="flexCheckDefault">
        <%= order.menu.name %>
        テーブル<%= order.tables_id %>から
        <%= order.quantity %>個の注文
      </label>
    </div>
  </li>
</ul>
<% end %>